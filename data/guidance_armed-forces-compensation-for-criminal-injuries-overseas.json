                                            "title":  "CIC(O) Scheme Rules 1 July 2017",
                                            "url":  "https://assets.publishing.service.gov.uk/media/5e25bf5eed915d1f1aeae13c/20170522-CICO_Scheme_Rules_2017_-_O.pdf"
                                        },
                                        {
                                            "accessible":  true,
                                            "alternative_format_contact_email":  "ddc-modinternet@mod.gov.uk",
                                            "attachment_type":  "file",
                                            "content_type":  "application/pdf",
                                            "file_size":  833615,
                                            "filename":  "CICO_0001_-_Online_version.pdf",
                                            "id":  "8300512",
                                            "number_of_pages":  10,
                                            "title":  "CIC(O) 001 Claim Form",
                                            "url":  "https://assets.publishing.service.gov.uk/media/67067f79e84ae1fd8592f092/CICO_0001_-_Online_version.pdf"
